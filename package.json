{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon ./index.js",
    "css": "postcss public/css/tailwind.css -o public/css/app.css --watch",
    "js": "webpack --watch",
    "dev": "concurrently \"npm run server\" \"npm run css\" \"npm run js\"",
    "db:importar" : "node ./seed/seeder.js -i",
    "db:eliminar" : "node ./seed/seeder.js -d"
  },
  "keywords": [
    "MVC",
    "Pud",
    "Tailwing",
    "MYSQL",
    "Sequelize"
  ],
  "author": "Frank Mariano",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.7",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.2",
    "nodemailer": "^6.9.1",
    "pug": "^3.0.2",
    "sequelize": "^6.29.0",
    "tippy.js": "^6.3.7"
  }
}
